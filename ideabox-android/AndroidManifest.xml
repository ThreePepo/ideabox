<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.taobao.ideabox"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10"/>
    <application android:label="@string/app_name" android:icon="@drawable/myspace">
        <activity  android:name=".android.activity.MyFileManager"  android:theme="@android:style/Theme.Dialog"/>
        <activity  android:name=".android.activity.DoneActivity"  android:theme="@android:style/Theme.Dialog"/>
        <activity android:name=".android.activity.Entrance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".android.activity.IdeaList"/>
        <activity android:name=".android.activity.IdeaCreateActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
</manifest>
